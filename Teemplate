local function Message(tit, tex, dur)
	game.StarterGui:SetCore("SendNotification", {Title = tit, Text = tex, Duration = dur})
end

wait(0)

Message("Join Nest Gang!","https://discord.gg/9N8vxZ5Www",5)

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Cryptonuis/untitled-ui/main/remaded%20cool%20ui"))()
local n = "NestHub | 2.1 | https://discord.gg/9N8vxZ5Www"
local Lib = Library:Window({
	Name = n,
	Color3.fromRGB(255, 0, 0)
})
local delcar = nil
UseAtMe = false
Damage = 0
Target = "N/A"
local plrs = {}
local function add()
	table.clear(plrs)
		for i,v in pairs(game.Players:GetChildren())	do
			table.insert(plrs, v.Name)
		end
end
local mre = nil
local mset = {
	id = 0,
	pitch = 1,
	volume = 10,
	looped = true
}
--—É–ª—å—è–≤–Ω–æ—Å–∫ —Ä–ø - –ª—É—á—à–∞—è —Ä–ø –∏–≥—Ä–∞üíì
local function LoadACS(TYPE)

	local function KillAll()
		for _,player in pairs(game.Players:GetPlayers()) do
			if UseAtMe == false then
				if player ~= game.Players.LocalPlayer then
					game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, math.huge, math.huge, 0)
				end
			else
				game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, math.huge, math.huge, 0)
			end
		end
	end

	local function GodAll()
		for _,player in pairs(game.Players:GetPlayers()) do
			if UseAtMe == false then
				if player ~= game.Players.LocalPlayer then
					game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, -math.huge, -math.huge, 0)
				end
			else
				game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, -math.huge, -math.huge, 0)
			end
		end
	end

	local function DamageAll()
		for _,player in pairs(game.Players:GetPlayers()) do
			if UseAtMe == false then
				if player ~= game.Players.LocalPlayer then
					game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, Damage, Damage, 0)
				end
			else
				game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, Damage, Damage, 0)
			end
		end
	end

	local function ExplodeAll()
		local Settings = {
			["ExplosiveHit"] = true,
			["ExPressure"] = 100,
			["ExpRadius"] = 100,
			["DestroyJointRadiusPercent"] = 100,
			["ExplosionDamage"] = math.huge,
		}

		for i,g in pairs(game.Players:GetChildren()) do
			if _G.UseAtMe == false then
				if g ~= game.Players.LocalPlayer then
					local v = g.Character.HumanoidRootPart
					game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				end
			else
				local v = g.Character.HumanoidRootPart
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
			end
		end
	end

	local function ExplodeMap()
		local Settings = {
			["ExplosiveHit"] = true,
			["ExPressure"] = math.huge,
			["ExpRadius"] = math.huge,
			["DestroyJointRadiusPercent"] = math.huge,
			["ExplosionDamage"] = math.huge,
		}

			for i,v in pairs(game.Workspace:GetDescendants()) do
				if v:IsA("BasePart") then
					game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				end
			end
		end

		local function Crash()
			local Settings = {
				["ExplosiveHit"] = true,
				["ExPressure"] = math.huge,
				["ExpRadius"] = math.huge,
				["DestroyJointRadiusPercent"] = math.huge,
				["ExplosionDamage"] = math.huge,
			}

			for i,v in pairs(game.Workspace:GetDescendants()) do
				if v:IsA("BasePart") then
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				game.ReplicatedStorage.ACS_Engine.Eventos.Hit:FireServer(v.Position, v, v.Position, Enum.Material.Plastic, Settings)
				end
			end
		end

		local function KillTarget(Target)
			for _,player in pairs(game.Players:GetPlayers()) do
					if player.Name == Target then
						game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, math.huge, math.huge, 0)
					end
			end
		end

		local function GodTarget(Target)
			for _,player in pairs(game.Players:GetPlayers()) do
					if player.Name == Target then
						game.ReplicatedStorage["ACS_Engine"].Eventos.Damage:FireServer(player.Character.Humanoid, -math.huge, -math.huge, 0)
					end
			end
		end
	
	
	

	if TYPE == 175 then
		local Tab = Lib:Tab({
			Name = "ACS"
		})
		
			local Section = Tab:Section({
			Name = "ACS 1.7.5 Hacks"
			
			})
			Section:Label({
				Name = "Some functions may not work."
			})

			Section:Label({
				Name = "---------------------------------SETTINGS---------------------------------"
			})

			Section:Toggle({
				Name = "UseAtMe",
				Default = false,
				Callback = function(State)
					UseAtMe = State
				end
			})

		add()
		
		Section:SearchBox({
			Name = "Choose Target",
			List = plrs,
			Default = Target,
			Callback = function(Value)
				Target = Value
			end
		})
	
		
		Section:Button({
			Name = "Update Target List [if not automatically updated]",
			Callback = function()
				add()
				SearchBox:Update("Choose Target")
			end
		})
		
			Section:Label({
				Name = "---------------------------------FUNCTIONS---------------------------------"
			})


			Section:Button({
				Name = "Kill All",
				Callback = function()
					KillAll()
				end
		})
		
		Section:Button({
			Name = "Kill Target",
			Callback = function()
				KillTarget(Target)
			end
		})

			Section:Button({
				Name = "God All",
				Callback = function()
					GodAll()
				end
		})
		
		Section:Button({
			Name = "God Target",
			Callback = function()
				GodTarget(Target)
			end
		})

			Section:Button({
				Name = "Explode All",
				Callback = function()
					ExplodeAll()
				end
			})

			Section:Button({
				Name = "Explode Map",
				Callback = function()
					ExplodeMap()
				end
			})

			Section:Button({
				Name = "Crash Server",
				Callback = function()
					Crash()
				end
			})

		
		
		
	elseif TYPE == 201 then

		end
end

local function LoadC6()
	local n1 = "Rev"
	local function Play()
		mre:FireServer('newSound', n1, game.SoundService, 'rbxassetid://'..mset.id, mset.pitch, mset.volume, mset.looped)
		mre:FireServer('playSound', n1)
	end

	local function Stop()
		mre:FireServer('newSound', n1, game.SoundService, 'rbxassetid://', 0, 0, false)
		mre:FireServer('playSound', n1)
	end

	local function StopAll()
		for i,v in pairs(game.SoundService:GetChildren()) do
			delcar:FireServer(v)
		end
	end

	local Tab = Lib:Tab({
		Name = "Music Player"
	})
	local Section = Tab:Section({
		Name = "---------------------------------SETTINGS---------------------------------"
	})

	Section:Toggle({
		Name = "Looped",
		Default = mset.looped,
		Callback = function(State)
			mset.looped = State
		end
	})

	Section:Box({
		Default = mset.id,
		Name = "ID",
		Callback = function(Value)
			mset.id = Value
		end
	})

	Section:Box({
		Default = mset.volume,
		Name = "Volume",
		Callback = function(Value)
			mset.volume = Value
		end
	})

	Section:Box({
		Default = mset.pitch,
		Name = "Pitch",
		Callback = function(Value)
			mset.pitch = Value
		end
	})


	Section:Label({
		Name = "---------------------------------FUNCTIONS---------------------------------"
	})

	Section:Button({
		Name = "Play Music",
		Callback = function()
			Play()
		end
	})

	Section:Button({
		Name = "Stop Music",
		Callback = function()
			Stop()
		end
	})

		Section:Button({
			Name = "Stop All Music",
			Callback = function()
				StopAll()
			end
		})
end


for i,v in pairs(game.Workspace:GetDescendants()) do
	if v.Name == 'AC6_FE_Sounds' and v.ClassName == 'RemoteEvent' then
		local n = game.HttpService:GenerateGUID(true)
		v:FireServer('newSound', n, game.SoundService, 'rbxassetid://', 0, 0, false)
		v:FireServer('playSound', n)
		wait(1)
		if game.SoundService:FindFirstChild(n) then
			mre = v
			LoadC6()
			v:FireServer('playSound', n)
			break
		else
		end
	end
end

local function LoadDeleteCar()
	
	
	local function KickAll()
		for i,v in pairs(game.Players:GetChildren()) do
			delcar:FireServer(v)
		end
	end
	
	local function KickOthers()
		for i,v in pairs(game.Players:GetChildren()) do
			if v ~= game.Players.LocalPlayer then
				delcar:FireServer(v) 
			end
		end
	end
	
	local function Custom(service)
		if service == "Workspace" then
			for i,v in pairs(game[service]:GetChildren()) do
				if not game.Players:FindFirstChild(v.Name) then
					delcar:FireServer(v)
				end
			end
		else
			for i,v in pairs(game[service]:GetChildren()) do
				delcar:FireServer(v)
			end
		end
	end
	
	local function KickTarget(Tar)
		for i,v in pairs(game.Players:GetChildren()) do
			if v.Name == Tar then
				delcar:FireServer(v)
			end
		end
	end
	
	local function FEB()
		local ev = delcar
		local player = game.Players.LocalPlayer
		local mouse = player:GetMouse()
		local ScreenGui = Instance.new("ScreenGui")
		local TextButton = Instance.new("TextButton")
		local On = Instance.new("StringValue")
		ScreenGui.Parent = player.PlayerGui
		ScreenGui.ResetOnSpawn = false
		TextButton.Parent = ScreenGui
		TextButton.BackgroundColor3 = Color3.new(0.784314, 0.784314, 0.784314)
		TextButton.BorderSizePixel = 0
		TextButton.Position = UDim2.new(0, 0, 0.455743879, 0)
		TextButton.Size = UDim2.new(0, 186, 0, 35)
		TextButton.Font = Enum.Font.SourceSans
		TextButton.Text = "FE Btools (Off)"
		TextButton.TextColor3 = Color3.new(0.27451, 0.27451, 0.27451)
		TextButton.TextScaled = true
		TextButton.TextSize = 14
		TextButton.TextWrapped = true
		On.Parent = TextButton
		On.Value = "Off"
		TextButton.MouseButton1Up:Connect(function()
			if On.Value == "Off" then
				On.Value = "On"
				TextButton.Text = "FE Btools (On)"
			else
				On.Value = "Off"
				TextButton.Text = "FE Btools (Off)"
			end
		end)
		mouse.Button1Up:Connect(function()
			if On.Value == "Off" then
			else
				if mouse.Target.Locked == true then
					ev:FireServer(mouse.Target)
				else
					ev:FireServer(mouse.Target)
				end
			end
		end)
	end
	local Tab = Lib:Tab({
			Name = "Game Control"
		})
	local Section = Tab:Section({
			Name = "Game control"
	})
	
	Section:Label({
		Name = "Some functions may not work."
	})
	
	Section:Label({
		Name = "---------------------------------SETTINGS---------------------------------"
	})
	
	add()
	
	local cur = nil
	
	Section:SearchBox({
		Name = "Target",
		List = plrs,
		Default = Target,
		Callback = function(Value)
			Target = Value
		end
	})


	Section:Button({
		Name = "Update Target List [if not automatically updated]",
		Callback = function()
			add()
			SearchBox:Update("Target")
		end
	})
	
	
		Section:Label({
			Name = "---------------------------------FUNCTIONS---------------------------------"
		})
		Section:Button({
			Name = "Kick All",
			Callback = function()
				KickAll()
			end
		})
		Section:Button({
			Name = "Kick Others",
			Callback = function()
				KickOthers()
			end
		})
		Section:Button({
			Name = "Kick Target",
			Callback = function(v)
				KickTarget(Target)
			end
	})
	
	
	Section:SearchBox({
		Name = "Choose Service",
		List = {
			"Workspace",
			"Lighting",
			"Teams",
			"SoundService",
			"Players",
			"StarterGui",
			"StarterPack",
			"ReplicatedStorage"
		},
		Default = "Workspace",
		Callback = function(Value)
			cur = Value
		end
	})
	
	Section:Button({
		Name = "Clear choosen service",
		Callback = function(Target)
			Custom(cur)
		end
	})
	
	local function DestroyHum()
		for i,v in pairs(game.Players:GetChildren()) do
			delcar:FireServer(v.Character:FindFirstChildOfClass("Humanoid"))
		end
	end	
	
	local function hd()
		if game.ReplicatedStorage:FindFirstChild("HDAdminClient") then
			delcar:FireServer(game.ReplicatedStorage:FindFirstChild("HDAdminClient"))
		else
		end
	end	
	
	local function NoTools()
		for i,v in pairs(game.Players:GetChildren()) do
			for _, n in pairs(v.Backpack:GetChildren()) do
				if n:IsA("Tool") then
					delcar:FireServer(n)
				end
			end
			for _, b in pairs(v.Character:GetChildren()) do
				if b:IsA("Tool") then
					delcar:FireServer(b)
				end
			end
			for _, s in pairs(game.StarterPack:GetChildren()) do
				if s:IsA("Tool") then
					delcar:FireServer(s)
				end
			end
		end
	end	
	
	local function DestroyChat()
		for i,v in pairs(game.Players:GetChildren()) do
			delcar:FireServer(game.ReplicatedStorage["DefaultChatSystemChatEvents"])
		end
	end	
	
	Section:Label({
		Name = "---------------------------------TROLLING---------------------------------"
	})
	
	Section:Button({
		Name = "Destroy Humanoids",
		Callback = function(v)
			DestroyHum()
		end
	})
	
	if game.ReplicatedStorage:FindFirstChild("HDAdminClient") then
		Section:Button({
			Name = "Destroy HD Admin",
			Callback = function(v)
				hd()
			end
		})

	end
	
	if game.ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents") then
	
	Section:Button({
		Name = "Break Chat",
		Callback = function(v)
			DestroyChat()
		end
		})
	end
	
	Section:Button({
		Name = "No Tools",
		Callback = function(v)
			NoTools()
		end
	})
	
	Section:Label({
		Name = "---------------------------------SCRIPTS---------------------------------"
	})
	
	Section:Button({
		Name = "Load FE Btools",
		Callback = function(Target)
			FEB()
		end
	})
	
	end


	for i,v in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
		if v.Name == "ACS_Engine" then
		if v:FindFirstChild("Eventos") then
			LoadACS(175)
			break
			elseif v:FindFirstChild("Events") then
			LoadACS(201)
			break
			end
		end
end


for i,v in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
	if v.Name == "DeleteCar" then
		delcar = v
		LoadDeleteCar()
		break
	end
end

local function DestroyUI()
	Library:UnLoad()
end

local function LoadCurrentGame()
	local id = game.PlaceId
	if id == 10158202520 then
		local function TP()
			if game.Players.LocalPlayer.Backpack:FindFirstChild("Detain") then
				game.Players.LocalPlayer.Backpack:FindFirstChild("Detain").Parent = game.Players.LocalPlayer.Character
			end	
				wait(0.1)
				local A_1 = "Cuff"
				local A_11 = "UnCuff"
				local Event = game.Players.LocalPlayer.Character.Detain.RemoteEvent
				for i,v in pairs(game.Players:GetChildren()) do
					if v.Name ~= game.Players.LocalPlayer.Name then
						local A_2 = v.Character.HumanoidRootPart
						Event:FireServer(A_1, A_2)
						wait(0.5)
						Event:FireServer(A_11, A_2)
						wait(0.2)
					end
			end
		end
		
		local Tab = Lib:Tab({
			Name = "Special Game"
		})
		local Section = Tab:Section({
			Name = "---------------------------------FUNCTIONS---------------------------------"
		})
		
		Section:Button({
			Name = "TP All Me [NEED CUFFS]",
			Callback = function()
				TP()
			end
		})
		
		local function ChangeTeam(c)
			local A_1 = c
			local Event = game:GetService("ReplicatedStorage").ChangeTeamEvent
			Event:FireServer(A_1)
		end
		
		local function Reset()
			local plr = game.Players.LocalPlayer
			local chr = plr.Character
			chr.Humanoid.Health = 0
		end
		
		local teams = {}
		
		for i,v in pairs(game.Teams:GetChildren()) do
			table.insert(teams, v.Name)
		end
		
		Section:SearchBox({
			Name = "Change Team",
			List = teams,
			Default = "N/A",
			Callback = function(Value)
				ChangeTeam(Value)
			end
		})
		
		Section:Button({
			Name = "Reset",
			Callback = function()
				Reset()
			end
		})
		
	elseif id == 1 then
	end
end

LoadCurrentGame()

local Tab = Lib:Tab({
	Name = "UI Settings"
})
